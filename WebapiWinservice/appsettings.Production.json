{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5006"
      },
      "HttpsDefaultCert": {
        "Url": "https://test.example.com:5003",
        "Certificate": {
          "Subject": "*.example.com",
          "Store": "My",
          "Location": "LocalMachine",
          "AllowInvalid": true
        }
      }
    }
  },
  "AllowedHosts": "192.168.130.16;test.example.com",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "CTX": "data source="
  },

  "WindowsServiceSettings": {
    "RunAs": "",
    "RunAsPassword": ""
  },

  "HostingSettings": {
    "UseDeveloperExceptionPage": false,
    "HttpsRedirection": true
  }

}